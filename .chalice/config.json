{
  "version": "2.0",
  "app_name": "pdf2image",
  "stages": {
    "dev": {
      "api_gateway_stage": "api",
      "lambda_memory_size": 2048,
      "lambda_timeout": 900,
      "layers": ["arn:aws:lambda:us-west-2:074561909539:layer:poppler:1"]
    }
  },
  "environment_variables": {
    "ORIGIN_BUCKET": "ztextract",
    "DESTINATION_BUCKET": "ztextract",
    "FMT": "jpeg",
    "DPI": "300"
  }
}
